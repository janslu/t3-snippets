---
layout: compress
---

<!doctype html>
<html lang="en">

  <head>

    {% assign site_title = site.title | default: site.name %}
    {% if site_title %}
      {% assign site_title = site_title | smartify | strip_html | strip_newlines | escape_once %}
    {% endif %}

    {% if page.title %}
      {% assign page_title = page.title | smartify | strip_html | strip_newlines | escape_once %}
    {% endif %}

    {% if site.github.build_revision %}
      {% assign build_version = site.github.build_revision %}
    {% else %}
      {% assign build_version = site.time | date: '%Y%m%d%H%M%S' %}
    {% endif %}

    {% capture theme_font_family_base %}-apple-system, BlinkMacSystemFont, "Segoe UI", Roboto, "Helvetica Neue", Arial, sans-serif{% endcapture %}

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">
    <title>{{ page_title }} &mdash; {{ site_title }}</title>

    {% if page.robots %}<meta name="robots" content="{{ page.robots }}">{% endif %}

    {% include head_links.liquid %}

    <style>
      body {
        min-height: 100vh !important;
      }
      .searchPage-heading {
        margin-top: 50px;
        margin-bottom: 0;
        font-family: {{ theme_font_family_base }};
        font-weight: normal;
        font-size: 18px;
      }
      .searchPage-body {
        margin: 30px 0 50px;
        font-family: {{ theme_font_family_base }};
      }
      .searchPage-form {
        margin-bottom: 30px;
      }
      .searchPage-results,
      .searchPage-results * {
        -webkit-box-sizing: content-box;
           -moz-box-sizing: content-box;
                box-sizing: content-box;
      }
      {% comment %}
      /**
      * Google custom search engine results
      * Overring styles for Google CSE default theme v2
      * https://cse.google.com/cse/style/look/v2/default.css
      */
      {% endcomment %}

      .gsc-control-cse,
      .gsc-control-cse .gsc-table-result,
      .gsc-completion-container,
      .gs-result .gs-title,
      .gs-result .gs-title *,
      .gs-result .gs-visibleUrl,
      .gs-snippet,
      .gsc-results .gsc-cursor-box {
        font-family: {{ theme_font_family_base }} !important;
      }

      .cse .gs-webResult .gs-visibleUrl,
      .gs-webResult .gs-visibleUrl,
      .cse .gs-webResult .gs-visibleUrl-short,
      .gs-webResult .gs-visibleUrl-short,
      .gs-imageResult .gs-visibleUrl,
      .gs-imageResult .gs-visibleUrl-short {
        font-weight: normal !important;
      }

      .cse .gsc-control-cse,
      .gsc-control-cse,
      .gsc-control-cse .gsc-table-result {
        display: block !important;
        width: auto !important;
      }

      .gsc-control-cse {
        padding: 1em 0 !important;
      }

      .gsc-above-wrapper-area {
        margin-bottom: 23px !important;
      }

      .gsc-result-info {
        padding-left: 0 !important;
      }

      .gsc-results,
      .gsc-webResult {
        display: block !important;
        padding: 0 !important;
        width: auto !important;
      }

      .gsc-webResult .gsc-result,
      .gsc-webResult.gsc-result {
        border: 0 !important;
      }

      .gsc-webResult .gsc-result {
        padding: 0 !important;
        margin-bottom: 25px !important;
      }

      .gs-result .gs-title,
      .gs-result .gs-title * {
        height: auto !important;
        font-size: 21px !important;
        text-decoration: none !important;
      }

      .gs-result .gs-title {
        font-weight: 600;
      }

      .gs-result .gs-title b{
        color: black !important;
      }

      .gs-result .gs-visibleUrl {
        margin: 3px 0 3px !important;
        font-size: 14px !important;
        color: rgba(0,0,0,0.44) !important;
      }

      .gs-snippet {
        margin-top: 0 !important;
        font-size: 15px !important;
      }

      .gsc-results .gsc-cursor-box {
        text-align: center !important;
      }

      .gsc-results .gsc-cursor-box .gsc-cursor {
        display: block !important;
        padding: 10px !important;
      }

      .gsc-results .gsc-cursor-box .gsc-cursor-page {
        padding: 4px 8px !important;
        border-radius: 4px;
      }

      {% comment %}
      /**
      * Do't show the following itmes on Google custom search engine results
      */
      {% endcomment %}

      .gsc-adBlock,
      .gcsc-branding,
      .gsc-thumbnail,
      .gsc-location {
        display: none !important;
      }
    </style>
  </head>

  <body>
    {% include navbar.liquid %}

    <section class="container">
      <div class="row">
        <div class="col-md-12">
          {{ content }}
        </div>
      </div>
    </section>

    {% include scripts.liquid %}
  </body>
</html>