---
layout: none
---
<feed xmlns="http://www.w3.org/2005/Atom">
<script/>
<link href="{{ site.baseurl }}/feed.xml" rel="self" type="application/atom+xml"/>
<link href="{{ site.baseurl }}" rel="alternate" type="text/html"/>
<updated>{{ site.time | date_to_xmlschema }}</updated>
<id>{{ site.baseurl }}</id>
<title type="html">{{ site.name | xml_escape }}</title>
<author>
    <name>{{ site.name | xml_escape }}</name>
    <uri>{{ site.author.uri | xml_escape}}</uri>
</author>
{% for snippet in site.snippets reversed limit:10%}
<entry>
    <title type="html">{{ snippet.title | smartify | xml_escape }}</title>
    <link href="{{ site.baseurl }}{{ snippet.url }}" rel="alternate" type="text/html" title="{{ snippet.title | xml_escape }}"/>
    <published>{{ snippet.date | date_to_xmlschema }}</published>
    <id>{{ site.baseurl }}{{ snippet.url | xml_escape}}</id>
    <author>
        <name>{{ snippet.author | xml_escape }}</name>
    </author>
    <summary type="html">
        {{ snippet.description | xml_escape }}
    </summary>
</entry>
{% endfor %}
</feed>